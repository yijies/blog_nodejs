{
  "name": "blog_nodejs",
  "version": "3.3.3",
  "description": "RESTful API service for Surmon.me blog",
  "author": {
    "name": "Surmon",
    "email": "surmon@foxmail.com",
    "url": "https://surmon.me"
  },
  "private": false,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yijies/blog_nodejs/issues"
  },
  "homepage": "https://github.com/yijies/blog_nodejs#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yijies/blog_nodejs.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "updategeodb": "node ./node_modules/geoip-lite/scripts/updatedb.js"
  },
  "dependencies": {
    "@nestjs/common": "^6.11.7",
    "@nestjs/core": "^6.11.7",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.3.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.11.7",
    "@nestjs/typeorm": "^6.3.1",
    "@typegoose/typegoose": "^6.3.2",
    "akismet-api": "^5.0.0",
    "ali-oss": "^6.5.1",
    "body-parser": "^1.19.0",
    "cache-manager": "^2.11.1",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.0",
    "csurf": "^1.11.0",
    "escape-html": "^1.0.3",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.1",
    "geoip-lite": "^1.4.0",
    "googleapis": "^47.0.0",
    "helmet": "^3.21.2",
    "hiredis": "^0.5.0",
    "js-base64": "^2.5.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "marked": "0.6.3",
    "mongoose": "^5.9.1",
    "mongoose-auto-increment": "^5.0.1",
    "mongoose-paginate": "https://github.com/surmon-china/mongoose-paginate",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.4.2",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rss": "^1.2.2",
    "rxjs": "^6.5.4",
    "shelljs": "^0.8.3",
    "simple-netease-cloud-music": "^0.5.0",
    "sitemap": "^5.1.0",
    "typeorm": "^0.2.22",
    "typescript": "^3.8.0",
    "wonderful-bing-wallpaper": "^0.3.4",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.13.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.11.7",
    "@types/ali-oss": "^6.0.4",
    "@types/express": "^4.17.2",
    "@types/jest": "25.1.2",
    "@types/lodash": "^4.14.149",
    "@types/marked": "0.6.5",
    "@types/mongodb": "^3.3.16",
    "@types/mongoose": "^5.7.1",
    "@types/mongoose-paginate": "^5.0.8",
    "@types/node": "^13.7.1",
    "@types/node-schedule": "^1.3.0",
    "@types/redis": "^2.8.15",
    "@types/rss": "^0.0.28",
    "@types/shelljs": "^0.8.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
